{
  "name": "bean-machine-cli",
  "version": "1.8.0",
  "description": "Source code to create and mint generative art via SoulMint",
  "main": "index.js",
  "bin": "index.js",
  "pkg": {
    "assets": [
      "layers/**/*",
      "node_modules/**/*",
      "src/**/*"
    ]
  },
  "scripts": {
    "chain": "./start-local-environment.sh",
    "build": "node index.js",
    "create_provenance": "node utils/art_engine/create_provenance.js",
    "create_wallet_edition_combo": "node utils/custom/create_wallet_edition_combo.js",
    "generate_metadata": "node utils/art_engine/generate_metadata.js",
    "mint": "node utils/soulmint/mint.js",
    "check_mints": "node utils/custom/check_mints.js",
    "check_mints_batch": "node utils/custom/check_mints_batch.js",
    "pixelate": "node utils/art_engine/pixelate.js",
    "preview_gif": "node utils/art_engine/preview_gif.js",
    "preview": "node utils/art_engine/preview.js",
    "rarity": "node utils/art_engine/rarity.js",
    "rarity_md": "node utils/custom/getRarity_fromMetadata.js",
    "rarity_rank": "node utils/custom/rarity_rank.js",
    "update_image_info": "node utils/custom/update_image_info.js",
    "update_metadata_info": "node utils/custom/update_metadata_info.js",
    "update_nft_info": "node utils/custom/update_nft_info.js",
    "generate_wallet": "hardhat generate",
    "wallet": "hardhat account",
    "deploy_local": "node utils/soulmint/deploy.js",
    "deploy_goerli": "hardhat run utils/soulmint/deploy.js --network goerli",
    "pin_files": "node utils/soulmint/pinFiles.js",
    "update_machine": "node utils/soulmint/startMachine.js"
  },
  "author": "Gadillac",
  "license": "MIT",
  "dependencies": {
    "@chainsafe/dappeteer": "2.4.1",
    "@openzeppelin/contracts": "^4.4.0",
    "@openzeppelin/contracts-upgradeable": "^4.5.2",
    "async-sema": "^3.1.1",
    "canvas": "^2.9.0",
    "chalk": "^4.1.0",
    "cids": "^1.1.6",
    "commander": "^7.1.0",
    "concurrently": "^6.0.0",
    "form-data": "^4.0.0",
    "getconfig": "^4.5.0",
    "gif-encoder-2": "^1.0.5",
    "go-ipfs": "^0.16.0",
    "graceful-fs": "^4.2.9",
    "inquirer": "^8.0.0",
    "ipfs-http-client": "50.1.2",
    "it-all": "^1.0.5",
    "json-colorizer": "^2.2.2",
    "keccak256": "^1.0.6",
    "merkletreejs": "^0.2.32",
    "multiaddr": "^8.1.2",
    "node-fetch": "^2.6.7",
    "puppeteer": "13.7.0",
    "sha1": "^1.1.1",
    "simple-get": "4.0.1",
    "uint8arrays": "^2.1.0"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.1",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "chai": "^4.3.3",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.0.31",
    "hardhat": "^2.1.1"
  }
}
